{
  "name": "NHSample",
  "version": "0.0.1",
  "private": true,
  "jest": {
    "scriptPreprocessor": "node_modules/react-native/jestSupport/preprocessor.js",
    "setupEnvScriptFile": "node_modules/react-native/jestSupport/env.js",
    "preprocessorIgnorePatterns": [
      "/node_modules/"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "packager/react-packager/src/Activity/"
    ],
    "testFileExtensions": [
      "es6",
      "js"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "es6"
    ],
    "unmockedModulePathPatterns": [
      "react",
      "react-native",
      "react-addons-test-utils",
      "react-native-search-bar",
      "react-native-router-flux",
      "promise",
      "source-map",
      "key-mirror",
      "immutable",
      "fetch",
      "redux",
      "redux-thunk",
      "fbjs"
    ]
  },
  "scripts": {
    "start": "react-native start",
    "test": "rm -rf ./node_modules/jest-cli/.haste_cache && jest "
  },
  "dependencies": {
    "react-native-camera": "git+https://github.com/lwansbrough/react-native-camera.git",
    "react-native-fbsdkcore": "0.0.8",
    "react-native-fbsdklogin": "0.0.8",
    "react-native-fbsdkshare": "0.0.8",
    "react-native-router-flux": "^2.3.9",
    "react-native-search-bar": "https://github.com/FuYaoDe/react-native-search-bar.git",
    "react-redux": "^4.4.0",
    "redux": "^3.3.1",
    "redux-promise": "^0.5.3",
    "redux-thunk": "^2.0.1"
  },
  "devDependencies": {
    "babel-core": "^6.7.2",
    "babel-jest": "^9.0.3",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react-native": "^1.5.2",
    "install": "^0.6.0",
    "jest-cli": "^0.9.2",
    "npm": "^3.8.2",
    "react": "^0.14.7",
    "react-addons-test-utils": "^0.14.7",
    "react-native": "^0.21.0",
    "redux-logger": "^2.6.1"
  }
}
